{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": [2, "always"],
        "camelcase": [2, {"properties": "never"}],
        "no-prototype-builtins": "off"
    }
}
